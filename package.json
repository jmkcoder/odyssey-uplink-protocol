{
    "name": "@jmkcoder/uplink-protocol",
    "version": "0.0.1",
    "description": "Odyssey Uplink Protocol",
    "main": "dist/cjs/index.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js",
            "types": "./dist/esm/index.d.ts"
        },
        "./uplink-auto-init": {
            "import": "./dist/esm/uplink-auto-init.js",
            "require": "./dist/cjs/uplink-auto-init.js",
            "types": "./dist/esm/uplink-auto-init.d.ts"
        },
        "./react": {
            "import": "./dist/esm/services/integration/react.js",
            "require": "./dist/cjs/services/integration/react.js",
            "types": "./dist/esm/services/integration/react.d.ts"
        },
        "./vue": {
            "import": "./dist/esm/services/integration/vue.js",
            "require": "./dist/cjs/services/integration/vue.js",
            "types": "./dist/esm/services/integration/vue.d.ts"
        },
        "./angular": {
            "import": "./dist/esm/services/integration/angular.js",
            "require": "./dist/cjs/services/integration/angular.js",
            "types": "./dist/esm/services/integration/angular.d.ts"
        },
        "./svelte": {
            "import": "./dist/esm/services/integration/svelte.js",
            "require": "./dist/cjs/services/integration/svelte.js",
            "types": "./dist/esm/services/integration/svelte.d.ts"
        },
        "./hooks": {
            "import": "./dist/esm/hooks.js",
            "require": "./dist/cjs/hooks.js",
            "types": "./dist/esm/hooks.d.ts"
        },
        "./auto-detect": {
            "import": "./dist/esm/services/integration/auto-detect.js",
            "require": "./dist/cjs/services/integration/auto-detect.js",
            "types": "./dist/esm/services/integration/auto-detect.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "build": "npm run clean && npm run build:umd && npm run build:cjs && npm run build:esm",
        "build:umd": "webpack --config webpack.lib.js --mode=production",
        "build:esm": "tsc -p tsconfig.esm.json",
        "build:cjs": "tsc -p tsconfig.cjs.json",        "clean": "rimraf dist",
        "prepare": "npm run build",
        "dev": "webpack serve --mode development",
        "create-package": "npm publish --access=public",
        "migrate-uplink": "node src/scripts/migrate-uplink.js",
        "demo": "powershell -ExecutionPolicy Bypass -File scripts/open-demo.ps1",
        "test:binding": "powershell -ExecutionPolicy Bypass -File scripts/test-two-way-binding.ps1",
        "react:demo": "powershell -ExecutionPolicy Bypass -File scripts/open-react-demo.ps1",
        "hooks:demo": "powershell -ExecutionPolicy Bypass -File scripts/open-hooks-demo.ps1"
    },
    "bin": {
        "migrate-uplink": "./dist/scripts/migrate-uplink.js"
    },
    "keywords": [
        "web-components",
        "ui",
        "components",
        "button"
    ],
    "author": "",
    "license": "ISC",
    "peerDependencies": {
        "@types/react": "^18.0.0 || ^19.0.0",
        "@types/react-dom": "^18.0.0 || ^19.0.0",
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0",
        "angular": "^1.8.3",
        "svelte": "^5.28.6",
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.16",
        "@types/react": "^19.1.3",
        "@types/react-dom": "^19.1.3",
        "autoprefixer": "^10.4.21",
        "css-loader": "^7.1.2",
        "esm": "^3.2.25",
        "html-webpack-plugin": "^5.6.3",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "lit": "^3.2.1",
        "mini-css-extract-plugin": "^2.9.2",
        "postcss": "^8.5.3",
        "postcss-loader": "^8.1.1",
        "postcss-webpack-plugin": "^1.0.2",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "remark-gfm": "^4.0.1",
        "rimraf": "^6.0.1",
        "sass": "^1.87.0",
        "sass-loader": "^16.0.5",
        "style-loader": "^4.0.0",
        "to-string-loader": "^1.2.0",
        "ts-jest": "^29.3.2",
        "ts-loader": "^9.5.2",
        "typescript": "^5.8.3",
        "webpack": "^5.98.0",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.1"
    }
}
